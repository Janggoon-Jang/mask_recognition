{
  "name": "Kaggle Python Dev",
  "image": "kaggle/python",
  "workspaceFolder": "/mask_recognition",
  "mounts": [
    "source=/mnt/c/projects/mask_recognition,target=/mask_recognition,type=bind"
  ],
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
  "postCreateCommand": "pip install -r /mask_recognition/requirements.txt"
}
